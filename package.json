{
  "name": "wfc-sculpture-stls",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "electron:build": "tsc -p electron/tsconfig.json",
    "electron:watch": "tsc -p electron/tsconfig.json -w",
    "electron:run": "wait-on http://localhost:5173 electron-dist/main.js && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron electron-dist/main.js",
    "build": "vite build",
    "start": "concurrently -k \"npm:dev\" \"npm:electron:watch\" \"npm:electron:run\"",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^22.10.1",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^33.2.1",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "three": "^0.169.0"
  }
}

